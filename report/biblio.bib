@article{allamigeon_tropical_2020,
	title = {Tropical spectrahedra},
	volume = {63},
	issn = {0179-5376, 1432-0444},
	url = {http://arxiv.org/abs/1610.06746},
	doi = {10.1007/s00454-020-00176-1},
	abstract = {We introduce tropical spectrahedra, deﬁned as the images by the nonarchimedean valuation of spectrahedra over the ﬁeld of real Puiseux series. We provide an explicit polyhedral characterization of generic tropical spectrahedra, involving principal tropical minors of size at most 2. One of the key ingredients is Denef–Pas quantiﬁer elimination result over valued ﬁelds. We obtain from this that the nonarchimedean valuation maps semialgebraic sets to semilinear sets that are closed. We also prove that, under a regularity assumption, the image by the valuation of a basic semialgebraic set is obtained by tropicalizing the inequalities which deﬁne it.},
	pages = {507--548},
	number = {3},
	journaltitle = {Discrete \& Computational Geometry},
	shortjournal = {Discrete Comput Geom},
	author = {Allamigeon, Xavier and Gaubert, Stéphane and Skomra, Mateusz},
	urldate = {2023-06-06},
	date = {2020-04},
	langid = {english},
	eprinttype = {arxiv},
	eprint = {1610.06746 [math]},
	keywords = {14T05 (Primary), 90C22, 14P10, 12J25 (Secondary), Mathematics - Algebraic Geometry, Mathematics - Combinatorics, Mathematics - Optimization and Control},
	file = {Allamigeon et al. - 2020 - Tropical spectrahedra.pdf:C\:\\Users\\coren\\Zotero\\storage\\EV24LDXT\\Allamigeon et al. - 2020 - Tropical spectrahedra.pdf:application/pdf},
