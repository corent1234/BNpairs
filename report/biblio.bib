	file = {ScienceDirect Full Text PDF:C\:\\Users\\coren\\Zotero\\storage\\QELRTJM7\\Allamigeon et al. - 2019 - The tropical analogue of the Helton–Nie conjecture.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\coren\\Zotero\\storage\\38F43HHW\\S0747717118300828.html:text/html},
